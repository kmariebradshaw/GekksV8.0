.prodrating {
  unicode-bidi: bidi-override;
  direction: rtl;
  display: inline-block;
}
.prodrating > span {
  display: inline-block;
  position: relative;
  width: 1.1em;
  color: #6DCDB8;  
}

.prodrating > span:nth-of-type(n + 2):before {
   content: "\2605";
   position: absolute;
   color: #6DCDB8; 
}

.prodrating > .pf:before {
   content: "\2605";
   position: absolute;
   color: #6DCDB8; 
   overflow: hidden; 
  	width: 45%; 
  -moz-transform: scaleX(-1);
  -o-transform: scaleX(-1);
  -webkit-transform: scaleX(-1);
  transform: scaleX(-1);
  filter: FlipH;
  -ms-filter: "FlipH";
  left: 0; 
  margin-left: 2px; 
}
.price.sale .subscript {
  text-decoration: line-through;
